<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="839.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.FuncionarioListController">
   <children>
      <Label text="Funcionários">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <padding>
            <Insets bottom="5.0" left="5.0" top="5.0" />
         </padding>
      </Label>
      <TableView fx:id="tableViewFuncionario">
        <columns>
          <TableColumn fx:id="tableColumnCpf" prefWidth="75.0" text="CPF" />
          <TableColumn fx:id="tableColumnNome" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="tableColumnNascimento" prefWidth="75.0" text="Nascimento" />
            <TableColumn fx:id="tableColumnSexo" prefWidth="75.0" text="Sexo" />
            <TableColumn fx:id="tableColumnSenha" prefWidth="75.0" text="Senha" />
            <TableColumn fx:id="tableColumnIdEmpresa" prefWidth="75.0" text="ID Empresa" />
            <TableColumn fx:id="tableColumnIdCargo" prefWidth="75.0" text="ID Cargo" />
            <TableColumn fx:id="tableColumnAtivo" prefWidth="75.0" text="Ativo" />
            <TableColumn fx:id="tableColumnGerenciamento" prefWidth="75.0" text="Gerente" />
            <TableColumn fx:id="tableColumnEditar" prefWidth="75.0" />
            <TableColumn fx:id="tableColumnRemover" prefWidth="75.0" />
        </columns>
      </TableView>
      <Button fx:id="btnNovoFuncionario" mnemonicParsing="false" onAction="#onBtnNovoFuncionarioAction" text="Novo Funcionário" textFill="#10c7ce">
         <VBox.margin>
            <Insets bottom="10.0" left="5.0" top="5.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</VBox>
