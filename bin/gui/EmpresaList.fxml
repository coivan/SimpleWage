<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="620.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.EmpresaListController">
   <children>
      <Label text="Gestão da empresa">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets left="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <TableView fx:id="tableViewEmpresa">
        <columns>
          <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tableColumnCnpj" prefWidth="75.0" text="CNPJ" />
            <TableColumn fx:id="tableColumnNome" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="tableColumnTelefone" prefWidth="75.0" text="Telefone" />
            <TableColumn fx:id="tableColumnEndereco" prefWidth="75.0" text="Endereço" />
            <TableColumn fx:id="tableColumnEmail" prefWidth="75.0" text="Email" />
            <TableColumn fx:id="tableColumnEditar" prefWidth="75.0" />
            <TableColumn fx:id="tableColumnRemover" prefWidth="75.0" />
        </columns>
      </TableView>
      <Button fx:id="btnAdicionarEmpresa" mnemonicParsing="false" onAction="#onBtnAdicionarEmpresaAction" text="Adicionar Empresa" textFill="#10c7ce">
         <VBox.margin>
            <Insets bottom="10.0" left="5.0" top="5.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</VBox>
